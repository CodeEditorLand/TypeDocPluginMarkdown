{
	"name": "typedoc-plugin-markdown",
	"version": "0.0.1",
	"private": false,
	"description": "A plugin for TypeDoc that enables TypeScript API documentation to be generated in Markdown.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/TypeDocPluginMarkdown#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/TypeDocPluginMarkdown/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/TypeDocPluginMarkdown.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "dist/index.js",
	"files": [
		"dist/"
	],
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prebuild": "ts-node scripts/build-resources",
		"build": "rm -rf dist && tsc",
		"build-and-test": "yarn run build && yarn run test",
		"docs": "yarn run build && yarn run docs:md && yarn run docs:html",
		"docs:html": "typedoc --plugin none --options ../../stub-project/typedoc.json --out ./docs/html",
		"docs:md": "typedoc --plugin typedoc-plugin-markdown --hasOwnDocument all --options ../../stub-project/typedoc.json --out ./docs/md",
		"markdownlint": "yarn run docs:md && markdownlint ./docs/md",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	}
}
